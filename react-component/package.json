  "icon": "images/react.png",
  "version": "1.0.0",
  "publisher": "jeremyrajan",
  "bugs": {
    "url": "https://github.com/jeremyrajan/vscode-exts/tree/master/react-component/issues"
  },
  "homepage": "https://github.com/jeremyrajan/vscode-exts/tree/master/react-component",
